{
  "connection": "default",
  "collectionName": "restaurants",
  "info": {
    "name": "restaurant"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "nom": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "plats": {
      "collection": "plat",
      "via": "restaurant"
    },
    "dailies": {
      "collection": "plat",
      "via": "restaurant"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    }
  }
}
